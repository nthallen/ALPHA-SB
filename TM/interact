  case `hostname` in
    easwhlpt3425080)
      # Nort's laptop:
      # ascenderdev=/dev/ttyS7
      # propmtrdev=/dev/ttyS3
      # slcandev=/dev/ttyS5
      # spatialdualdev=/dev/ttyS3
      ;;
    EASWHLPT3425022)
      # Marco's laptop
      propmtrdev=
      slcandev=/dev/ttyS3;;
    spi2)
      # ascenderdev=/dev/ttyUSB0
      # propmtrdev=/dev/ttyUSB0
      # spatialdualdev=/dev/ttyUSB1
      tmseriodev=/dev/ttyUSB0
      # slcandev=/dev/ttyACM0
      ;;
    [Ss]pi3)
      #propmtrdev=/dev/ttyUSB0
      #tmseriodev=/dev/ttyUSB2
      #ascenderdev=/dev/ttyUSB1
      #spatialdualdev=/dev/ttyUSB3
      #slcandev=/dev/ttyACM0
      #socketcan=can0
      ;;
    [Ss]pi4)
      # ascenderdev=/dev/ttyUSB0
      # propmtrdev=/dev/ttyUSB0
      # spatialdualdev=/dev/ttyUSB1
      tmseriodev=/dev/ttyUSB0
      # slcandev=/dev/ttyACM0
      ;;
    nortvmu22)
      tmseriodev=/dev/ttyUSB0
      ;;
    *)
      propmtrdev=/dev/ttyUSB0
      slcandev=/dev/ttyACM0;;
  esac

if [ -n "$socketcan" ]; then
  sudo ip link set $socketcan type can bitrate 50000
  sudo ip link set up $socketcan
fi

  Launch      memo   memo -o $Experiment.log -l4
              [ -z "$launch_err" ] &&
                msg "Running script interact"
[ -c "$slcandev" ] &&
  Launch      -      subbusd_slcan -n CAN -p $slcandev
[ -n "$socketcan" ] &&
  Launch      -      subbusd_CAN -n CAN
  Launch      tm_bfr bfr -H bfr: -S:
  Launch      -TMC-  lgr -N `mlf_find LOG` -n lgr
  Launch      tm_gen scopexcol
  Launch      cmd    scopexsrvr -H srvr: -S: -v
[ -c "$tmseriodev" ] &&
  Launch      -TMC-  tmserioext -d $tmseriodev -b 115200 -v -f
  # Was 230400
[ -n "$relay_ext" ] &&
  Launch      -TMC-  tm_ip_export -H Relay:$relay_ext -E scopex.sat
  Launch      -      tmdf
[ -c "$propmtrdev" ] &&
  Launch      -      PropMtr -p $propmtrdev
[ -c "$spatialdualdev" ] &&
  Launch      -      SpatialDual -p $spatialdualdev -v
[ -c "$ascenderdev" ] &&
  Launch      -      Ascender -p $ascenderdev -v
  Launch      -TMC-  scopexalgo
