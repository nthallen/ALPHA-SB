  " Demand:" (PMC_Right_RPM_Demand,7) RPM;
  " Meas:"   (PMC_Right_RPM,7)        RPM;
  Current:;
  " Demand:" (PMC_Right_I_Demand,6)   Amps;
  " Meas:"   (PMC_Right_I_Measured,6) Amps;
  Pos_Set:   (PMC_Right_Pos_SP,11);
  " Demand:" (PMC_Right_Pos_Demand,11);
