<package> CAN
<include> "scopex_can.h"

<defs>
  subbuspp *CAN;
  bool CAN_initialized = false;
  void CAN_init() {
    int subfunc;
    CAN = new subbuspp("subbusd", "CAN");
    subfunc = CAN->load();
    if (subfunc > 0) {
      msg(0, "CAN subbusd reported subfunction %d", subfunc);
      CAN_Initialized = true;
    } else {
      msg(MSG_ERROR, "CAN subbusd load failed: %d", subfunc);
    }
  }
<init>
  CAN_init();
