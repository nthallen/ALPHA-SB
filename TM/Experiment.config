Experiment=scopex
# DPi1 in Arlington:
# FlightNode=192.168.7.59
# SPi2 in Arlington:
# FlightNode=192.168.7.77
# FlightNode=spi3
# [ -z "$FlightNode" ] && RUNFILE=interact
case `pwd` in
  /home/scopex/Sim)
    HomeDir=/home/scopex/Sim
    RUNFILE=runfile.sim;;
  /home/scopex)
    HomeDir=/home/scopex
    FlightNode=spi3;;
  /home/flight)
    HomeDir=/home/scopex
    FlightNode=spi3;;
  *) echo "flight.sh: pwd = `pwd`";;
esac
RunType=ask
RunLog=saverun.log
Extractions=scopexengext
